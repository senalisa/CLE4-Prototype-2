ces":{"src/shark.ts":"import * as PIXI from \"pixi.js\";\nimport { Game } from \"./game\";\n\nexport class Hero extends PIXI.Sprite {\n\n  //GLOBALS\n  private speed: number = 0;\n  private game: Game;\n\n  //CONSTRUCTOR\n  constructor(texture: PIXI.Texture, game: Game) {\n\n    super(texture);\n    this.game = game;\n\n    this.x = 1500\n    this.y = 400\n    this.scale.set(-1, 1);\n\n    window.addEventListener(\"keydown\", (e: KeyboardEvent) => this.onKeyDown(e));\n    window.addEventListener(\"keyup\", (e: KeyboardEvent) => this.onKeyUp(e));\n\n  }\n\n  //KEYBOARD\n  onKeyDown(e: KeyboardEvent): any {\n    if (e.key === \"ArrowUp\") {\n      this.speed = -5;\n    }\n    if (e.key === \"ArrowDown\") {\n      this.speed = 5;\n    }\n  }\n  onKeyUp(e: KeyboardEvent): any {\n    if (e.key === \"ArrowUp\" || e.key === \"ArrowDown\") {\n      this.speed = 0;\n    }\n  }\n\n  //ANIMATION\n  public update() {\n    this.y += this.speed;\n\n    this.keepInScreen();\n  }\n\n  //KEEP IN SCREEN\n  private keepInScreen() {\n    if (this.getBounds().left > this.game.pixi.screen.right) {\n      this.x = this.game.pixi.screen.right;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"d55a481de06279d053d0aec0e2eca1cc","cacheData":{"env":{}}}